# Runtime profile (dev, homelab, vps)
ODIN_PROFILE=dev

# Log level (trace, debug, info, warn, error)
RUST_LOG=info

# Data directory (default: /var/odin)
# ODIN_DIR=/var/odin

# TUI profile (core, legacy)
# ODIN_TUI_PROFILE=core

# Acknowledge guardrails for mutating operations (set to 'yes' to enable)
# ODIN_GUARDRAILS_ACK=yes
